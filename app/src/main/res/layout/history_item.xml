<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/group_historry_list"
    android:layout_width="fill_parent"
    android:layout_height="50dp"
    android:gravity="left|center_vertical"
    android:layout_marginTop="50dp"
    android:orientation="horizontal">
    <TextView
        android:id="@+id/matchId"
        android:textSize="@dimen/hisrecord_text_size"
        android:layout_width="@dimen/hisrecord_widht_item"
        android:layout_height="@dimen/hisrecord_height_answer"
        android:layout_gravity="center"
        />
    <TextView
        android:id="@+id/projectId"
        android:textSize="@dimen/hisrecord_text_size"
        android:layout_width="@dimen/hisrecord_widht_answer_mode"
        android:layout_height="@dimen/history_answer_item_heigth"
        android:layout_gravity="center"
        />
    <TextView
        android:id="@+id/startDate"
        android:textSize="@dimen/hisrecord_game_date"
        android:layout_width="@dimen/hisrecord_height_answer"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView
        android:id="@+id/rectime"
        android:textSize="@dimen/hisrecord_text_size"
        android:layout_width="@dimen/hisrecord_height_answer"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView
        android:id="@+id/memtime"
        android:textSize="@dimen/hisrecord_text_size"
        android:layout_width="@dimen/hisrecord_height_answer"
        android:layout_height="wrap_content"
        android:gravity="center"
        />
    <TextView
        android:id="@+id/score"
        android:textSize="@dimen/hisrecord_text_size"
        android:layout_width="@dimen/hisrecord_widht_answer_mode"
        android:layout_height="@dimen/hisrecord_height_answer"
        android:gravity="center"
        />
    <TextView
        android:id="@+id/ranking"
        android:textSize="@dimen/hisrecord_text_size"
        android:layout_width="@dimen/hisrecord_widht_answer_mode"
        android:layout_height="@dimen/hisrecord_height_answer"
        android:gravity="center"
        />

</TableRow>

